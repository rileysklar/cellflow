{
  "name": "ai-optimized-starter-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "next build",
    "clean": "npm run type-check && npm run lint:fix && npm run format:write",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,cjs,mjs,jsx,json,html,css}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,cjs,mjs,jsx,json,html,css}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky",
    "start": "next start",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx,cjs,mjs,js,jsx,json,html,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx,cjs,mjs,js,jsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@clerk/backend": "^1.25.2",
    "@clerk/nextjs": "^6.12.5",
    "@clerk/themes": "^2.2.21",
    "@hookform/resolvers": "^4.1.3",
    "@next/bundle-analyzer": "^15.2.2",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.40.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "framer-motion": "^12.5.0",
    "lucide-react": "^0.482.0",
    "next": "^15.2.2",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.5",
    "react": "^19.0",
    "react-dom": "^19.0",
    "react-hook-form": "^7.54.2",
    "sonner": "^2.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@next/eslint-plugin-next": "^15.2.2",
    "@tailwindcss/postcss": "^4.0.14",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22",
    "@types/react": "^19.0",
    "@types/react-dom": "^19.0",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.22.0",
    "eslint-config-next": "15.2.2",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.10",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.14",
    "tailwindcss-animate": "^1.0.7",
